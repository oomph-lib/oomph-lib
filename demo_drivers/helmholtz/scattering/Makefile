	@rm -f adaptive_unstructured_scattering$(EXEEXT)
	$(AM_V_CXXLD)$(adaptive_unstructured_scattering_LINK) $(adaptive_unstructured_scattering_OBJECTS) $(adaptive_unstructured_scattering_LDADD) $(LIBS)

